<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>tests</servlet-name>
        <servlet-class>testing.tests</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Verify</servlet-name>
        <servlet-class>users.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>users.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterUser</servlet-name>
        <servlet-class>users.RegisterUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Auth</servlet-name>
        <servlet-class>users.Auth</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createpost</servlet-name>
        <servlet-class>posts.createpost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>users.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>submitpost</servlet-name>
        <servlet-class>posts.submitpost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>showposts</servlet-name>
        <servlet-class>posts.showposts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Navigate</servlet-name>
        <servlet-class>users.Navigate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>show</servlet-name>
        <servlet-class>posts.show</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>showservices</servlet-name>
        <servlet-class>posts.showservices</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>showservice</servlet-name>
        <servlet-class>posts.showservice</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profile</servlet-name>
        <servlet-class>users.profile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tests</servlet-name>
        <url-pattern>/tests</url-pattern>
    </servlet-mapping>
    <context-param>
        <param-name>jsp-inheritance-prefix</param-name>
        <param-value>/WEB-INF/layouts/</param-value>
    </context-param>
    <context-param>
        <param-name>jsp-inheritance-suffix</param-name>
        <param-value>.jsp</param-value>
    </context-param>
    <servlet-mapping>
        <servlet-name>RegisterUser</servlet-name>
        <url-pattern>/RegisterUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Auth</servlet-name>
        <url-pattern>/Auth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createpost</servlet-name>
        <url-pattern>/createpost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>submitpost</servlet-name>
        <url-pattern>/submitpost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>showposts</servlet-name>
        <url-pattern>/showposts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Navigate</servlet-name>
        <url-pattern>/Navigate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>show</servlet-name>
        <url-pattern>/show</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>showservices</servlet-name>
        <url-pattern>/showservices</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>showservice</servlet-name>
        <url-pattern>/showservice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
